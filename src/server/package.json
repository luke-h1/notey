{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsnd --respawn --transpile-only --no-notify --ignore-watch node_modules src/index.ts",
    "watch": "tsnd --respawn --transpile-only --no-notify --ignore-watch node_modules src/index.ts",
    "start": "NODE_ENV=production node dist",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e ."
  },
  "dependencies": {
    "mongodb": "^3.6.5",
    "cors": "2.8.5",
    "express": "^4.17.3",
    "apollo-server": "3.10.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.0",
    "typescript": "4.9.5",
    "pino": "8.4.1",
    "pino-pretty": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "18.7.17",
    "@types/jsonwebtoken": "8.5.9",
    "gen-env-types": "1.3.4",
    "@types/mongodb": "4.0.7",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "@types/bcryptjs": "2.4.2",
    "ts-node-dev": "2.0.0"
  }
}
